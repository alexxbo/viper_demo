<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="net.omisoft.aborovskoy.viperdemo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:name="net.omisoft.aborovskoy.viperdemo.app.App"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="net.omisoft.aborovskoy.viperdemo.ui.detail.DetailActivity">
        </activity>
        <activity android:name="net.omisoft.aborovskoy.viperdemo.ui.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="net.omisoft.aborovskoy.viperdemo.ui.main.MainActivity"/>
    </application>

</manifest>